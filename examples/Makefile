.PHONY: all hello fxbmp fxpad cellophane psg bg7up spr7up scsi clean

all: hello fxbmp fxpad cellophane psg bg7up spr7up scsi

hello:
	make -C $@
fxbmp:
	make -C $@
fxpad:
	make -C $@
cellophane:
	make -C $@
psg:
	make -C $@
bg7up:
	make -C $@
spr7up:
	make -C $@
scsi:
	make -C $@

clean:
	make -C hello clean
	make -C fxbmp clean
	make -C fxpad clean
	make -C cellophane clean
	make -C psg clean
	make -C bg7up clean
	make -C spr7up clean
	make -C scsi clean

